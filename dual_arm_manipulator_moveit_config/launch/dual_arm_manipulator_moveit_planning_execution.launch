<?xml version="1.0" ?>
<launch>
  <arg name="pipeline" default="ompl" />
  <arg name="debug" default="false" />
  <arg name="use_rviz" default="true" />

  <rosparam command="load" file="$(find dual_arm_manipulator_moveit_config)/config/joint_names.yaml"/>

  <include file="$(find dual_arm_manipulator_moveit_config)/launch/planning_context.launch" />

  <include file="$(find dual_arm_manipulator_moveit_config)/launch/move_group.launch">
    <arg name="allow_trajectory_execution" value="true"/>
    <arg name="fake_execution" value="false"/>
    <arg name="info" value="true"/>
    <arg name="debug" value="$(arg debug)"/>
    <arg name="pipeline" value="$(arg pipeline)"/>
  </include>

  <include file="$(find dual_arm_manipulator_moveit_config)/launch/moveit_rviz.launch" if="$(arg use_rviz)">
    <arg name="rviz_config" value="$(find dual_arm_manipulator_moveit_config)/launch/moveit.rviz"/>
    <arg name="debug" value="$(arg debug)"/>
  </include>

</launch>